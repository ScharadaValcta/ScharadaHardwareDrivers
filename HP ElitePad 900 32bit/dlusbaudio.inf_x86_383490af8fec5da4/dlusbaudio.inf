Needs=KS.Registration, WDMAUDIO.Registration, mssysfx.CopyFilesAndRegister
CopyFiles=_CopyFiles_sys_x64
AddReg=_KSAddReg,_KSAddReg_x64
;AddProperty=_KSAddProperty

[_Install.ntamd64.HW]
AddReg = _AddReg_HW, _AddReg_SD_OUT, _AddReg_SD_IN

[_Install.ntamd64.Services]
AddService = %S_ServiceName%, 0x00000002, _AddService_x64, _EventLog_x64

[_AddService_x64]
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_DEMAND_START
ErrorControl   = 1                  ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\%S_DriverName%_x64.sys
AddReg = _AddReg_MediaCategories

[_EventLog_x64]
AddReg=_EventLogAddReg_x64

[_EventLogAddReg_x64]
HKR,,EventMessageFile,%REG_EXPAND_SZ%,"%%SystemRoot%%\System32\IoLogMsg.dll;%%SystemRoot%%\System32\drivers\%S_DriverName%_x64.sys"
HKR,,TypesSupported,  %REG_DWORD%, 7


;******************************************************************************
; Registry sections
;------------------------------------------------------------------------------
[_AddReg_HW]
; enable workaround: swap CENTER and LFE channels
HKR,,EnableSwapCenterLfeWorkaround,  %REG_DWORD%, 1
; enable watchdog for IN stream
;HKR,,EnableWatchdogStreamingRestartIn,  %REG_DWORD%, 1
; enable watchdog for OUT stream
;HKR,,EnableWatchdogStreamingRestartOut,  %REG_DWORD%, 1
; configure watchdog timer interval (in milliseconds)
;HKR,,WatchdogTimerInterval,  %REG_DWORD%, 1000
; configure delay after jack connection state changes are reported (in milliseconds)
;HKR,,UpdateAllFiltersWorkItemDelay,  %REG_DWORD%, 500
; enable fidelity test workaround
;HKR,,FidelityTestWorkaround,  %REG_DWORD%, 1


; OUT sound devices setup (playback)
[_AddReg_SD_OUT]
; NOTE: All entries in this section are optional. If present, each setting overwrites the driver's default behavior.

; sound device category GUID (overrides driver-internal default which is GUID.Speaker)
;HKR, "sd_out_00", "PinCategoryGuid", %REG_SZ%, %GUID.Line%
; custom name. NOTE: On Vista/W7 this does not work for KSNODETYPE_SPEAKER category.
;HKR, "sd_out_00", "PinNameGuid",   %REG_SZ%, %GUID.CustomName1%   ; used on Vista/W7
;HKR, "sd_out_00", "DisplayNameXP", %REG_SZ%, %TEXT.CustomName1%   ; used on XP only

; sequence of subkeys that generates an array of jack description items, see also http://msdn.microsoft.com/en-us/library/dd316543(VS.85).aspx
; NOTE: settings FirstChannel, ChannelCount and SpeakerPositions are mandatory, other settings are optional
; item 0: L+R
HKR, "sd_out_00\jd_00", "FirstChannel", %REG_DWORD%, 0
HKR, "sd_out_00\jd_00", "ChannelCount", %REG_DWORD%, 2
HKR, "sd_out_00\jd_00", "SpeakerPositions", %REG_DWORD%, 0x00000003   ; SPEAKER_FRONT_LEFT|SPEAKER_FRONT_RIGHT  (see ksmedia.h)
HKR, "sd_out_00\jd_00", "ConnectionType", %REG_DWORD%, %eConnType3Point5mm%
HKR, "sd_out_00\jd_00", "GeoLocation", %REG_DWORD%, %eGeoLocRear%
HKR, "sd_out_00\jd_00", "GenLocation", %REG_DWORD%, %eGenLocPrimaryBox%
HKR, "sd_out_00\jd_00", "PortConnection", %REG_DWORD%, %ePortConnJack%
HKR, "sd_out_00\jd_00", "ColorR", %REG_DWORD%, 0    ; GREEN
HKR, "sd_out_00\jd_00", "ColorG", %REG_DWORD%, 255
HKR, "sd_out_00\jd_00", "ColorB", %REG_DWORD%, 0
; item 1: C+LFE
HKR, "sd_out_00\jd_01", "FirstChannel", %REG_DWORD%, 2
HKR, "sd_out_00\jd_01", "ChannelCount", %REG_DWORD%, 2
HKR, "sd_out_00\jd_01", "SpeakerPositions", %REG_DWORD%, 0x0000000C   ; SPEAKER_FRONT_CENTER|SPEAKER_LOW_FREQUENCY  (see ksmedia.h)
HKR, "sd_out_00\jd_01", "ConnectionType", %REG_DWORD%, %eConnType3Point5mm%
HKR, "sd_out_00\jd_01", "GeoLocation", %REG_DWORD%, %eGeoLocRear%
HKR, "sd_out_00\jd_01", "GenLocation", %REG_DWORD%, %eGenLocPrimaryBox%
HKR, "sd_out_00\jd_01", "PortConnection", %REG_DWORD%, %ePortConnJack%
HKR, "sd_out_00\jd_01", "ColorR", %REG_DWORD%, 255    ; ORANGE
HKR, "sd_out_00\jd_01", "ColorG", %REG_DWORD%, 165
HKR, "sd_out_00\jd_01", "ColorB", %REG_DWORD%, 0
; item 2: SL+SR
HKR, "sd_out_00\jd_02", "FirstChannel", %REG_DWORD%, 4
HKR, "sd_out_00\jd_02", "ChannelCount", %REG_DWORD%, 2
HKR, "sd_out_00\jd_02", "SpeakerPositions", %REG_DWORD%, 0x00000600   ; SPEAKER_SIDE_LEFT | SPEAKER_SIDE_RIGHT  (see ksmedia.h)
HKR, "sd_out_00\jd_02", "ConnectionType", %REG_DWORD%, %eConnType3Point5mm%
HKR, "sd_out_00\jd_02", "GeoLocation", %REG_DWORD%, %eGeoLocRear%
HKR, "sd_out_00\jd_02", "GenLocation", %REG_DWORD%, %eGenLocPrimaryBox%
HKR, "sd_out_00\jd_02", "PortConnection", %REG_DWORD%, %ePortConnJack%
HKR, "sd_out_00\jd_02", "ColorR", %REG_DWORD%, 0    ; BLACK
HKR, "sd_out_00\jd_02", "ColorG", %REG_DWORD%, 0
HKR, "sd_out_00\jd_02", "ColorB", %REG_DWORD%, 0


; IN sound devices setup (record)
[_AddReg_SD_IN]
; NOTE: All entries in this section are optional. If present, each setting overwrites the driver's default behavior.

; sound device category GUID (overrides driver-internal default which is GUID.Line)
;HKR, "sd_in_00", "PinCategoryGuid", %REG_SZ%, %GUID.Line%
; custom name. NOTE: On Vista/W7 this does not work for KSNODETYPE_SPEAKER category.
;HKR, "sd_in_00", "PinNameGuid",   %REG_SZ%, %GUID.CustomName2%   ; used on Vista/W7
;HKR, "sd_in_00", "DisplayNameXP", %REG_SZ%, %TEXT.CustomName2%   ; used on XP only

; sequence of subkeys that generates an array of jack description items, see also http://msdn.microsoft.com/en-us/library/dd316543(VS.85).aspx
; NOTE: settings FirstChannel, ChannelCount and SpeakerPositions are mandatory, other settings are optional
; item 0: L+R
HKR, "sd_in_00\jd_00", "FirstChannel", %REG_DWORD%, 0
HKR, "sd_in_00\jd_00", "ChannelCount", %REG_DWORD%, 2
HKR, "sd_in_00\jd_00", "SpeakerPositions", %REG_DWORD%, 0x00000003   ; SPEAKER_FRONT_LEFT|SPEAKER_FRONT_RIGHT  (see ksmedia.h)
HKR, "sd_in_00\jd_00", "ConnectionType", %REG_DWORD%, %eConnType3Point5mm%
HKR, "sd_in_00\jd_00", "GeoLocation", %REG_DWORD%, %eGeoLocRear%
HKR, "sd_in_00\jd_00", "GenLocation", %REG_DWORD%, %eGenLocPrimaryBox%
HKR, "sd_in_00\jd_00", "PortConnection", %REG_DWORD%, %ePortConnJack%
HKR, "sd_in_00\jd_00", "ColorR", %REG_DWORD%, 250    ; PINK
HKR, "sd_in_00\jd_00", "ColorG", %REG_DWORD%, 175
HKR, "sd_in_00\jd_00", "ColorB", %REG_DWORD%, 190


; sound device name strings
[_AddReg_MediaCategories]
; If custom names are to be used, register these below.
; NOTE: This section should be empty if no custom names are used in [_AddReg_SD_OUT] or [_AddReg_SD_IN].
;HKLM,%MediaCategories%\%GUID.CustomName1%, Name,    %REG_SZ%,     %TEXT.CustomName1%
;HKLM,%MediaCategories%\%GUID.CustomName1%, Display, %REG_BINARY%, 00,00,00,00
;HKLM,%MediaCategories%\%GUID.CustomName2%, Name,    %REG_SZ%,     %TEXT.CustomName2%
;HKLM,%MediaCategories%\%GUID.CustomName2%, Display, %REG_BINARY%, 00,00,00,00



;******************************************************************************
; interface registration
;------------------------------------------------------------------------------
[_Install.ntx86.Interfaces]
AddInterface=%KSCATEGORY_AUDIO%,%KSNAME_Topology%
[_Install.ntamd64.Interfaces]
AddInterface=%KSCATEGORY_AUDIO%,%KSNAME_Topology%


;******************************************************************************
; make the driver available to the Multimedia system
;------------------------------------------------------------------------------
[_KSAddReg]
HKR,,AssociatedFilters,,"wdmaud,swmidi,redbook"
HKR,Drivers,SubClasses,,"wave,midi,mixer,aux"

HKR,Drivers\wave\wdmaud.drv,Driver,,wdmaud.drv
HKR,Drivers\midi\wdmaud.drv,Driver,,wdmaud.drv
HKR,Drivers\mixer\wdmaud.drv,Driver,,wdmaud.drv
HKR,Drivers\aux\wdmaud.drv,Driver,,wdmaud.drv

HKR,Drivers\wave\wdmaud.drv,Description,,%S_WdmAudDescription%
HKR,Drivers\midi\wdmaud.drv,Description,,%S_WdmAudDescription%
HKR,Drivers\mixer\wdmaud.drv,Description,,%S_WdmAudDescription%
HKR,Drivers\aux\wdmaud.drv,Description,,%S_WdmAudDescription%

HKR,,SetupPreferredAudioDevices,3,01,00,00,00

[_KSAddReg_x86]
HKR,,Driver,,%S_DriverName%.sys

[_KSAddReg_x64]
HKR,,Driver,,%S_DriverName%_x64.sys


;******************************************************************************
; Add properties (Vista only)
;------------------------------------------------------------------------------
