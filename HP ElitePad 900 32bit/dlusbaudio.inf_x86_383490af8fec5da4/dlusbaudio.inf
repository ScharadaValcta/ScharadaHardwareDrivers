;DeviceBrandingIcon,,,,"%12%\%S_DriverName%,-200"

;Place your URL here
;DeviceVendorWebSite,,,,"http://www.displaylink.com/" 



;******************************************************************************
; Copy file sections
;------------------------------------------------------------------------------
[_CopyFiles_sys_x86]
dlusbaudio.sys,,,0x00000004

[_CopyFiles_sys_x64]
dlusbaudio_x64.sys,,,0x00000004


;******************************************************************************
; Destination Directories
;------------------------------------------------------------------------------
[DestinationDirs]
DefaultDestDir  = 12  ; %windir%\system32\drivers
_CopyFiles_sys_x86 = 12
_CopyFiles_sys_x64 = 12

;******************************************************************************
; Disk Layout
;------------------------------------------------------------------------------
[SourceDisksNames.x86]
1=%S_DiskName%,,0
[SourceDisksNames.amd64]
1=%S_DiskName%,,0

[SourceDisksFiles.x86]
dlusbaudio.sys=1
[SourceDisksFiles.amd64]
dlusbaudio_x64.sys=1



